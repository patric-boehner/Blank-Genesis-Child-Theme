        "typography": {
            "customFontSize": false,
            "customFontStyle": true,
            "customFontWeight": true,
            "customLineHeight": false,
            "customTextDecorations": true,
            "customTextTransforms": true,
            "dropCap": false,
            "fontFamilies": [
                {
                    "fontFamily": "'Source Sans Pro', sans-serif",
                    "slug": "font-priamry",
                    "name": "Primary Font"
                }
            ],
            "fontSizes": [
                {
                    "slug": "tiny",
                    "size": "16px",
                    "name": "Tiny"
                },
                {
