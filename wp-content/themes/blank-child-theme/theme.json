{
    "version": 1,
    "settings": {

        "layout": {
            "contentSize": "var(--wp--custom--width--responsive)",
            "wideSize": "var(--wp--custom--width--full)"
        },

        "color": {
            "custom": false,
            "customDuotone": false,
            "customGradient": false,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": [
                {
                    "slug": "blue",
                    "color": "#004b96",
                    "name": "Blue"
                },
                {
                    "slug": "light-blue",
                    "color": "#d5e1ee",
                    "name": "Light Blue"
                },
                {
                    "slug": "green",
                    "color": "#30643A",
                    "name": "Green"
                },
                {
                    "slug": "light-green",
                    "color": "#D1E6D4",
                    "name": "Light Green"
                },
                {
                    "slug": "dark-gray",
                    "color": "#333",
                    "name": "Dark Gray"
                },
                {
                    "slug": "light-gray",
                    "color": "#eee",
                    "name": "Light Gray"
                },
                {
                    "slug": "white",
                    "color": "#fff",
                    "name": "White"
                }
            ]
        },

        "typography": {
            "customFontSize": false,
            "customFontStyle": true,
            "customFontWeight": true,
            "customLineHeight": false,
            "customTextDecorations": true,
            "customTextTransforms": true,
            "dropCap": false,
            "fontFamilies": [
                {
                    "fontFamily": "'Source Sans Pro', sans-serif",
                    "slug": "font-priamry",
                    "name": "Primary Font"
                }
            ],
            "fontSizes": [
                {
                    "slug": "tiny",
                    "size": "var(--wp--custom--font-size--tiny)",
                    "name": "Tiny"
                },
                {
                    "slug": "small",
                    "size": "var(--wp--custom--font-size--small)",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "var(--wp--custom--font-size--normal)",
                    "name": "Normal"
                },
                {
                    "slug": "large",
                    "size": "var(--wp--custom--font-size--max-large)",
                    "name": "Large"
                },
                {
                    "slug": "extra-large",
                    "size": "var(--wp--custom--font-size--max-extra-large)",
                    "name": "H3 Heading"
                },
                {
                    "slug": "huge",
                    "size": "var(--wp--custom--font-size--max-huge)",
                    "name": "H2 Heading"
                },
                {
                    "slug": "gigantic",
                    "size": "var(--wp--custom--font-size--max-gigantic)",
                    "name": "H1 Heading"
                }
            ]
        },

        "blocks": {
            "core/button": {
                "border": {
                    "customRadius": false
                }
            }
        },

        "custom": {
            "fontFamily": {
                "primary": "'Source Sans Pro', sans-serif",
                "secondary": "'Source Sans Pro', sans-serif"
            },
            "fontWeight": {
                "normal": "400",
                "bold": "600"
            },
            "body": {
                "font-family": "var(--wp--custom--font-family--secondary)",
                "font-weight": "var(--wp--custom--font-weight--normal)",
                "line-heigh": "1.35"
            },
            "color": {
                "foreground": "var(--wp--preset--color--dark-gray)",
                "background": "var(--wp--preset--color--white)",
                "primary": "var(--wp--preset--color--blue)",
                "secondary": "var(--wp--preset--color--green)"
            },
            "fontSize": {
                "tiny": "calc( (16/16) * 1rem)",
                "small": "calc( (18/16) * 1rem)",
                "normal": "calc( (20/16) * 1rem)",
                "medium": "calc( (22/16) * 1rem)",
                "large": "calc( (24/16) * 1rem)",
                "extra-large": "calc( (31/16) * 1rem)",
                "huge": "calc( (39/16) * 1rem)",
                "gigantic": "calc( (49/16) * 1rem)",
                "max-large": "clamp( var(--wp--custom--font-size--medium), 4vw, var(--wp--custom--font-size--large))",
                "max-extra-large": "clamp( var(--wp--custom--font-size--large), 4vw, var(--wp--custom--font-size--extra-large))",
                "max-huge": "clamp( var(--wp--custom--font-size--extra-large), 4vw, var(--wp--custom--font-size--huge))",
                "max-gigantic": "clamp( var(--wp--custom--font-size--huge), 4vw, var(--wp--custom--font-size--gigantic))"
            },
            "heading": {
                "size-h1": "var(--wp--custom--font-size--max-gigantic)",
                "size-h2": "var(--wp--custom--font-size--max-huge)",
                "size-h3": "var(--wp--custom--font-size--max-extra-large)",
                "size-h4": "var(--wp--custom--font-size--max-large)",
                "size-h5": "var(--wp--preset--font-size--small)",
                "size-h6": "var(--wp--preset--font-size--tiny)",
                "font-family": "var(--wp--custom--font-family--primary)",
                "text-transform": "uppercase",
                "font-weight": "var(--wp--custom--font-weight--bold)",
                "line-heigh": "1.2"
            },
            "border": {
                "color": "var(--wp--custom--color--foreground)"
            },
            "icon": {
                "normal": "calc( (20/16) * 1rem)",
                "medium": "calc( (28/16) * 1rem)",
                "small": "calc( var(--wp--custom--icon--medium) / 2)",
                "large": "calc( var(--wp--custom--icon--medium) * 2)"
            },
            "spacing": {
                "x-small": "5px",
                "small": "15px",
                "medium": "30px",
                "medium-large": "60px",
                "large": "90px",
                "x-large": "120px",
                "side": "8%",
                "content-area": "var(--wp--custom--spacing--x-large)",
                "row": "var(--wp--custom--spacing--large)",
                "group-padding": "var(--wp--custom--spacing--medium-large)"
            },
            "width": { 
                "full": "1400px",
                "content": "800px",
                "small": "600px",
                "max-width": "var(--wp--custom--width--full)",
                "responsive": "var(--wp--custom--width--full)"
            },
            "grid": {
                "gap": "var(--wp--custom--spacing--medium)"
            }
        }

    },

    "styles": {

        "typography": {
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontFamily": "var(--wp--custom--font-family--primary)",
			"lineHeight": "1.2",
            "fontWeight": "var(--wp--custom--font-weight--normal)"
        },

        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--green)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h-1)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h-2)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h-3)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h-4)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h-5)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h-6)"
                }
            }
        },

        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--dark-gray)"
        }
        
    },

    "templateparts": [
    ]

}