                    "slug": "huge",
                    "size": "39px",
                    "name": "H2 Heading"
                },
                {
                    "slug": "gigantic",
                    "size": "49px",
                    "name": "H1 Heading"
                },
                {
                    "slug": "max-31",
                    "size": "clamp( 24px, 6vw, 31px)",
                    "name": "Max Extra Large"
                },
                {
                    "slug": "max-39",
                    "size": "clamp( 31px, 6vw, 39px)",
                    "name": "Max Huge"
                },
                {
                    "slug": "max-49",
                    "size": "clamp( 39px, 6vw, 49px)",
                    "name": "Max Gigantic"
                }
            ]
        },

        "custom": {
            "fontFamily": {
                "primary": "'Source Sans Pro', sans-serif",
                "secondary": "'Source Sans Pro', sans-serif"
            },
            "fontWeight": {
                "normal": "400",
                "bold": "600"
            },
            "body": {
                "font-family": "var(--wp--custom--font-family--secondary)",
                "font-weight": "var(--wp--custom--font-weight--normal)",
                "line-heigh": "1.425"
            },
            "color": {
                "foreground": "var(--wp--preset--color--dark-gray)",
                "background": "var(--wp--preset--color--white)",
                "primary": "var(--wp--preset--color--blue)",
                "secondary": "var(--wp--preset--color--green)"
            },
            "heading": {
                "size-h1": "var(--wp--preset--font-size--gigantic)",
                "size-h2": "var(--wp--preset--font-size--huge)",
                "size-h3": "var(--wp--preset--font-size--extra-large)",
                "size-h4": "var(--wp--preset--font-size--large)",
                "size-h5": "var(--wp--preset--font-size--small)",
                "size-h6": "var(--wp--preset--font-size--tiny)",
                "font-family": "var(--wp--custom--font-family--primary)",
                "text-transform": "uppercase",
                "font-weight": "var(--wp--custom--font-weight--bold)",
                "line-heigh": "1.2"
            },
            "border": {
                "color": "var(--wp--custom--color--foreground)"
            },
            "icon": {
                "normal": "28px",
                "small": "calc( var(--wp--custom--icon--normal) / 2)",
                "large": "calc( var(--wp--custom--icon--normal) * 2)"
            },
            "spacing": {
                "x-small": "5px",
                "small": "15px",
                "medium": "30px",
                "large": "90px",
                "x-large": "120px",
                "side": "8%",
                "content-area": "var(--wp--custom--spacing--x-large)",
                "row": "var(--wp--custom--spacing--large)",
                "group-padding": "var(--wp--custom--spacing--small)"
            },
            "width": {
                "full": "1400px",
                "content": "800px",
                "small": "600px",
                "max-width": "var(--wp--custom--width--full)",
                "responsive": "var(--wp--custom--width--full)"
            },
            "grid": {
                "gap": "var(--wp--custom--spacing--medium)"
            }
        }

    },

    "styles": {

        "typography": {
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontFamily": "var(--wp--custom--font-family--primary)",
			"lineHeight": "1.2",
            "fontWeight": "var(--wp--custom--font-weight--normal)"
        },

        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--green)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h1)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h2)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h3)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h4)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h5)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--size-h6)"
                }
            }
        },

        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--dark-gray)"
        }
        
    },

    "templateparts": [
    ]

